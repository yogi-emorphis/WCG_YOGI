{
    "extends": "airbnb",
    "rules": {
      "func-names": 0,
      "no-console": 0,
      "strict": 0,
      "no-underscore-dangle": 0,
      "array-callback-return": 0,
      "arrow-parens": ["warn", "as-needed"],
      "arrow-body-style": ["warn", "as-needed"],
      "linebreak-style": "off"
    },
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    }
}